{
  "expo": {
    "name": "birabuddy",
    "slug": "birabuddy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "birabuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Find people to drink with - Only for students! (this is not a dating app :)",
    "primaryColor": "#1E1E1E",
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "url": "https://u.expo.dev/61232958-c524-4b7e-97c2-3a40115be165"
    },
    "runtimeVersion": "1.0.1",
    "ios": {
      "supportsTablet": false,
      "deploymentTarget": "13.4",
      "bundleIdentifier": "com.caglaryildirim.birabuddy2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera to scan beer labels and take photos",
        "NSLocationWhenInUseUsageDescription": "This app uses location to find nearby breweries and bars",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.caglaryildirim.birabuddy2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E1E1E"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1E1E1E"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61232958-c524-4b7e-97c2-3a40115be165"
      }
    },
    "owner": "caglaryildirim"
  }
}