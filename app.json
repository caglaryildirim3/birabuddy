{
  "expo": {
    "name": "BiraBuddy",
    "slug": "birabuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "birabuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "üni öğrencileri için birabuddy bulma uygulaması-şimdilik sadece boğaziçi öğrencileri",
    "primaryColor": "#86945fff",
    "platforms": [
      "ios",
      "android"
    ],
     "updates": {
      "url": "https://u.expo.dev/61232958-c524-4b7e-97c2-3a40115be165"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caglaryildirim.birabuddy",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera to scan beer labels and take photos",
        "NSLocationWhenInUseUsageDescription": "This app uses location to find nearby breweries and bars",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.caglaryildirim.birabuddy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#D2691E"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#D2691E"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61232958-c524-4b7e-97c2-3a40115be165"
      }
    },
    "owner": "caglaryildirim"
  }
}
